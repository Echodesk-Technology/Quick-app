{
  "name": "quickjs",
  "version": "1.0.0",
  "description": "Create Quick App",
  "private": "true",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "quickinstall": "npm install",
    "serve": "concurrently --kill-others \"npm run start\" \"npm run develop\"",
    "develop": "webpack --config webpack.dev.js",
    "build": "npm install --dev && webpack --config webpack.prod.js",
    "build:css": "tailwind build src/assets/css/build.css -o src/assets/css/main.css"
  },
